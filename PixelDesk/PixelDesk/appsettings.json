{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "OpenWeatherApi": {
    "Url": "https://api.openweathermap.org/data/2.5/weather",
    "ApiKey": "<API_KEY>",
    "City": "<CITY>"
  },
  "MQTT": {
    "Server": "<MQTT_SERVER>",
    "Port": "1883",
    "User": "<MQTT_USERNAME",
    "Password": "<MQTT_PASSWORD>",
    "DeviceId": "PixelDeskApp",
    "IntercomTopic": "devices/SP001/publish"
  }
}